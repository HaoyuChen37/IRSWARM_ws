<!-- launchfile.launch -->
<launch>
  <!-- 启动相机节点 -->
  <node name="cam1" pkg="cam" type="cam1.py" output="screen"/>
  <node name="cam2" pkg="cam" type="cam2.py" output="screen"/>
  <node name="cam3" pkg="cam" type="cam3.py" output="screen"/>
  <node name="cam4" pkg="cam" type="cam4.py" output="screen"/>

  <!-- 启动控制小车的节点 -->
  <node name="auto_control" pkg="cam" type="auto_control.py" output="screen"/>

</launch>
